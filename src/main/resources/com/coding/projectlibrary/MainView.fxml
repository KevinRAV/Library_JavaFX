<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.coding.projectlibrary.MainController">
   <children>
      <HBox prefHeight="327.0" prefWidth="600.0" spacing="70.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="btnLibrary" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Library">
               <HBox.margin>
                  <Insets left="80.0" top="100.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnImc" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="IMC">
               <HBox.margin>
                  <Insets top="100.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnSoldier" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Soldier">
               <HBox.margin>
                  <Insets top="100.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
